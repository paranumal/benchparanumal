#elif p_N==7
#define p_NelementsPerBlk 1
#else
#define p_NelementsPerBlk 1
#endif

#else
#if p_N==1
#define p_NelementsPerBlk 3
#elif p_N==2
#define p_NelementsPerBlk 4
#elif p_N==3
#define p_NelementsPerBlk 4
#elif p_N==4
#define p_NelementsPerBlk 1
#elif p_N==5
