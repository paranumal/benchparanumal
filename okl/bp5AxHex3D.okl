#define bp5AxHex3Dv2 bp5AxHex3D
#else
#if (OCCA_USE_CUDA==1||OCCA_USE_DPCPP==1) && p_N>8
#define bp5AxHex3Dv0 bp5AxHex3D
#else
#define bp5AxHex3Dv1 bp5AxHex3D
#endif
